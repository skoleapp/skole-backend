msgid "Mentor"
msgstr "Mentori"

#: skole/utils/constants.py:177
msgid "Bachelor"
msgstr "Kandidaatti"

#: skole/utils/constants.py:178
msgid "Master"
msgstr "Maisteri"

#: skole/utils/constants.py:179
msgid "Doctor"
msgstr "Tohtori"

#: skole/utils/constants.py:180
msgid "Professor"
msgstr "Professori"

#: skole/utils/constants.py:192
msgid "username"
msgstr "käyttäjänimi"

#: skole/utils/constants.py:193
msgid "email"
msgstr "sähköposti"

