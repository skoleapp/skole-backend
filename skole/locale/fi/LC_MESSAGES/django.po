msgid "Mentor"
msgstr "Mentori"

#: skole/utils/constants.py:153
msgid "Master"
msgstr "Maisteri"

#: skole/utils/constants.py:154
msgid "Doctor"
msgstr "Tohtori"

#: skole/utils/constants.py:155
msgid "Professor"
msgstr "Professori"

